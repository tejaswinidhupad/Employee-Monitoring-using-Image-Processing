package main;

import java.io.IOException;
import java.io.PrintWriter;
import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;

import javax.servlet.ServletConfig;
import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

/**
 * Servlet implementation class save
 */
@WebServlet("/save")
public class save extends HttpServlet {
	private static final long serialVersionUID = 1L;

    /**
     * Default constructor. 
     */
	//database variables
	Connection con = null;
	PreparedStatement ps;
	ResultSet rs;
    public save() {
        // TODO Auto-generated constructor stub
    }

	/**
	 * @see Servlet#init(ServletConfig)
	 */
	public void init(ServletConfig config) throws ServletException {
		try
		{
			
			con = DoConnectionSQL.getConnection();
		}
		catch(Exception e1)
		{
			e1.printStackTrace();
		}
	}

	/**
	 * @see HttpServlet#doPost(HttpServletRequest request, HttpServletResponse response)
	 */
	protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		// TODO Auto-generated method stub

		Email=request.getParameter("Email");
		Mobile=request.getParameter("Mobile");
		NoPerson=Integer.parseInt(request.getParameter("NoOfPerson"));
		Name1=request.getParameter("Name1");
		AddadharNo1=request.getParameter("aad_no1");
		Name2=request.getParameter("Name2");
		AddadharNo2=request.getParameter("aad_no2");		
		Name3=request.getParameter("Name3");
		AddadharNo3=request.getParameter("aad_no3");
		Name4=request.getParameter("Name4");
		AddadharNo4=request.getParameter("aad_no4");
		Name5=request.getParameter("Name5");
		AddadharNo5=request.getParameter("aad_no5");
		
		PrintWriter out = response.getWriter();
		
		
	}

}
